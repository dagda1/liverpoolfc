{
  "name": "liverpoolfc",
  "version": "0.1.0",
  "private": true,
  "description": "Liverpool queries",
  "type": "module",
  "module": "index.js",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "clean": "rm -rf ./packages/*/{tsconfig.*tsbuildinfo,dist}",
    "build": "turbo run build",
    "ithealth:codegen": "pnpm --filter @ithealth/frontend codegen",
    "lint": "turbo run lint",
    "prepare": "tsx ./scripts/prepare.ts",
    "preview": "concurrently \"pnpm start:mock-graphql-server\" \"cross-env NODE_ENV=production pnpm build && cross-env NODE_ENV=development  HARBOUR_ENVIRONMENT=dev pnpm --filter @harbour/frontend preview\"",
    "release": "pnpm run prerelease && changeset publish && git push --follow-tags && git status && pnpm -r publish",
    "start:frontend": "pnpm build && concurrently \"pnpm start:mock-graphql-server\" \"cross-env NODE_ENV=development pnpm --filter @harbour/frontend dev\"",
    "test": "cross-env turbo run test:ci",
    "version": "npm version"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@cutting/assert": "^0.4.0",
    "@urql/core": "6.0.1",
    "@urql/exchange-graphcache": "8.1.0",
    "date-fns": "^4.1.0",
    "immer": "^11.1.0",
    "json-schema": "^0.4.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router": "7.11.0",    "urql": "5.0.1",
    "use-debounce": "^10.0.6",
    "use-immer": "^0.11.0",
    "xior": "^0.8.2",
    "zod": "^4.2.1",
    "zustand": "5.0.9"
  },
  "devDependencies": {
    "@cutting/eslint-config": "^5.5.0",
    "@cutting/tsconfig": "^4.44.0",
    "@cutting/useful-types": "^4.45.0",
    "@eslint/compat": "^2.0.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript-eslint/eslint-plugin": "8.50.0",
    "@typescript-eslint/parser": "8.50.0",
    "@vitejs/plugin-react-swc": "^4.2.2",
    "@vitest/coverage-v8": "^4.0.16",
    "concurrently": "9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "hast-util-to-jsx-runtime": "2.3.6",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.7.4",
    "tsx": "^4.21.0",
    "turbo": "^2.7.1",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vite-plugin-checker": "^0.12.0",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vitest": "^4.0.16",
    "wait-on": "^9.0.3"
  },
  "packageManager": "pnpm@10.18.1",
  "volta": {
    "node": "22.19.0",
    "npm": "11.5.2",
    "pnpm": "10.26.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint -c ./eslint.config.mjs --fix --warn-ignored=false"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.2.7",
      "@types/react-dom": "19.2.3",
      "react": "19.2.3",
      "react-dom": "19.2.3"
    },
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@swc/core",
      "esbuild",
      "msw"
    ],
    "ignoredBuiltDependencies": [
      "puppeteer"
    ]
  }
}
